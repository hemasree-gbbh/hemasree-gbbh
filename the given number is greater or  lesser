#include <stdio.h>

int main() {
    int n;
    printf("Enter a num:");
    scanf("%d",&n);
    if(n>10)
    {
   printf("The given number is greater than 10");
   }
    else
   {
       printf("The given num is less than 10");
   } 
   if (n=10)
   {
       printf("The given num is equal to 10");
   }
    return 0;
} 
