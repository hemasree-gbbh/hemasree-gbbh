#include <stdio.h>

int main() {
    int arr[30],i,n,sum=0;
    printf("How many students are in the class?");
scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("Enter the student marks:");
        scanf("%d",&arr[i]);
    
sum=sum+arr[i];
}
printf("Sum of the marks of the students are %d:",sum);
}
